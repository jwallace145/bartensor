{
    "names": [
        "Banana Daiquiri"
    ],
    "ingredients": [
        "1 1/2 oz  Light rum",
        "1 tblsp  Triple sec",
        "1  Banana",
        "1 1/2 oz  Lime juice",
        "1 tsp  Sugar",
        "1  Cherry"
    ],
    "method": [
        "Champagne flute",
        "Pour all ingredients into shaker with ice cubes",
        "Shake well",
        "Strain in chilled cocktail glass"
    ],
