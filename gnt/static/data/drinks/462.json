{
    "names": [
        "Sherry Eggnog"
    ],
    "ingredients": [
        "2 oz cream  Sherry",
        "1 tsp  Powdered sugar",
        "1 whole  Egg",
        "Milk",
        "Nutmeg"
    ],
    "method": [
        "Collins glass",
        "Shake sherry, powdered sugar, and egg with ice and strain into a collins glass",
        "Fill with milk and stir",
        "Sprinkle nutmeg on top and serve"
    ],
