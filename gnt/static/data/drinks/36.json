{
    "names": [
        "Absolut Summertime"
    ],
    "ingredients": [
        "1 1/2 oz  Absolut Citron",
        "3/4 oz  Sweet and sour",
        "1/2 oz  Sprite",
        "3 oz  Soda water",
        "1 slice  Lemon"
    ],
    "method": [
        "Collins glass",
        "Add all ingredients except lemon to shaker filled with ice",
        "Cover and shake vigorously",
        "Strain contents into ice filled collins glass",
        "Garnish with lemon"
    ],
