    "ingredients": [
        "2 oz  Blended whiskey",
        "1/2 oz  Benedictine",
        "Juice of 1/4  Lemon",
        "Juice of 1/2  Lime",
        "1 slice  Lemon",
        "1 slice  Lime"
    ],
    "method": [
        "Whiskey sour glass",
        "Shake all ingredients (except slices of lemon and lime) with ice and strain into a whiskey sour glass",
        "Decorate with the slices of lemon and lime and serve"
