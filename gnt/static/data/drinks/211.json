{
    "names": [
        "Creme de Menthe"
    ],
    "ingredients": [
        "8 cups  Sugar",
        "6 cups  Water",
        "1 pint  Grain alcohol",
        "1 oz pure  Peppermint extract",
        "1 tblsp green  Food coloring"
    ],
    "method": [
        "Collins glass",
        "Bring sugar and water to a boil and simmer for 10 minutes",
        "Cool",
        "Add the remaining ingredients and stir",
        "Cover and let ripen for 1 month"
    ],
