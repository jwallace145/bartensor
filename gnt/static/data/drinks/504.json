{
    "names": [
        "Tia-Maria"
    ],
    "ingredients": [
        "1 cup  Water",
        "3/4-1 cup  Brown sugar",
        "4 tsp  Coffee",
        "1 cup  Rum",
        "4 tsp  Vanilla extract"
    ],
    "method": [
        "Collins glass",
        "Boil water, sugar and coffe for 10 mins and let cool",
        "Add rum and vanilla",
        "Put in clean bottle(s) and leave for 1 week before using"
    ],
    "picture": "http://www.thecocktaildb.com/images/media/drink/sih81u1504367097.jpg"
}