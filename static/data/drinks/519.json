{
    "names": [
        "Victory Collins"
    ],
    "ingredients": [
        "1 1/2 oz  Vodka",
        "3 oz  Lemon juice",
        "3 oz unsweetened  Grape juice",
        "1 tsp  Powdered sugar",
        "1 slice  Orange"
    ],
    "method": [
        "Collins glass",
        "Shake all ingredients (except orange slice) with ice and strain into a collins glass over ice cubes",
        "Add the slice of orange and serve"
    ],
