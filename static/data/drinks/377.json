{
    "names": [
        "Mojito #3"
    ],
    "ingredients": [
        "1/2 handful  Mint",
        "3 cl  Lemon juice",
        "1/8 L Jamaican  Dark rum",
        "1/8 L  Club soda",
        "8 drops  Angostura bitters"
    ],
    "method": [
        "Collins glass",
        "Put mint with lemon juice in a glas, mash the mint with a spoon, ice, rum & fill up with club soda",
        "Top it with angostura"
    ],
