{
    "names": [
        "Atomic Lokade"
    ],
    "ingredients": [
        "5 oz  Lemonade",
        "1 oz  Vodka",
        "1/2 oz  Blue Curacao",
        "1/2 oz  Triple sec",
        "Sugar",
        "Ice"
    ],
    "method": [
        "Collins glass",
        "In a shaker, place lemonade, vodka, blue curacao, and triple sec together",
        "Shake with ice and strain into glass",
        "Add sugar to taste"
    ],
