    "ingredients": [
        "1 1/2 oz  Light rum",
        "1 1/2 tsp  Triple sec",
        "1 tblsp  Lime juice",
        "1 tsp  Sugar",
        "1  Egg white",
        "1  Mint"
    ],
    "method": [
        "Old-fashioned glass",
        "Combine and shake all ingredients (except mint) with ice and strain into an old-fashioned glass over ice cubes",
        "Add the sprig of mint and serve"
    ],
