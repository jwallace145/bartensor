        "Mississippi Planters Punch"
    ],
    "ingredients": [
        "1 oz  Brandy",
        "1/2 oz  Light rum",
        "1/2 oz  Bourbon",
        "Juice of 1/2  Lemon",
        "1 tblsp  Powdered sugar",
        "Carbonated water"
    ],
    "method": [
        "Collins glass",
        "Shake all ingredients (except carbonated water) with ice and strain into a collins glass over ice cubes",
        "Fill with carbonated water, stir, and serve"
